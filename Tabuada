#include <stdio.h>
#include <stdlib.h>

/*
tabuada de 1 a 10
*/
int Tabuada (int Numero) {
      int resultado;
      int i;
      
    
     for (i=1;i<11;i++) { 
    printf (" %d x %d = %d \n",i, Numero, i * Numero);
     }
  return resultado;
  
}

int main()
{
    int Numero;
    
    printf("Digite um nÃºmero: ");
    scanf ("%d", &Numero);
    
    
Tabuada (Numero);
    return 0;
}
